{
  "name": "@rajatjain/count-up",
  "version": "1.1.0",
  "description": "A lightweight and high-performance vanilla JS library to animate numbers. Supports scroll-based and manual controls.",
  "main": "dist/count-up.min.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/count-up.min.js"
    }
  },
  "scripts": {
    "build": "terser index.js -o dist/count-up.min.js --module -c -m --comments /Copyright/"
  },
  "files": [
    "dist",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rajat-XR/npm-count-up.git"
  },
  "keywords": [
    "count-up",
    "counter",
    "animation",
    "number",
    "scroll",
    "vanilla-js",
    "lightweight",
    "animate",
    "odometer"
  ],
  "author": "rajatjain",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Rajat-XR/npm-count-up/issues"
  },
  "homepage": "https://github.com/Rajat-XR/npm-count-up#readme",
  "devDependencies": {
    "terser": "^5.24.0"
  }
}